public class Array {
    public static void main(String[] args) {
        int[] ints = new int[] {2, 4, 5, 6, 9};
        int first = ints[0];
        int last = ints[ints.length - 1];
        ints[0] = last;
        ints[ints.length - 1] = first;
        int result = ints[0] + ints[ints.length / 2];
        System.out.print(result);
    }
}